// 完整数组功能测试
函数 主() {
    // 测试数组字面量
    让 数组1 = [1, 2, 3, 4, 5]
    打印("数组1: " + 数组1)
    
    // 测试数组索引访问
    让 元素 = 数组1[0]
    打印("数组1[0]: " + 元素)
    
    // 测试数组长度
    让 长度 = 数组1.长度()
    打印("数组1长度: " + 长度)
    
    // 测试数组添加元素
    数组1.添加(6)
    打印("添加6后的数组1: " + 数组1)
    
    // 测试数组删除元素
    数组1.删除(2)
    打印("删除索引2后的数组1: " + 数组1)
    
    // 测试空数组
    让 空数组 = []
    打印("空数组: " + 空数组)
    
    // 测试数组中的不同类型
    让 混合数组 = [1, "hello", 3.14]
    打印("混合数组: " + 混合数组)
}

主()