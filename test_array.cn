// 测试数组功能

// 定义主函数
函数 主() {
    // 测试数组字面量
    让 数组1 = [1, 2, 3, 4, 5];
    打印("数组1: " + 数组1);
    
    // 测试数组索引访问
    让 元素 = 数组1[0];
    打印("数组1[0]: " + 元素);
    
    // 测试数组长度
    让 长度 = 数组1.长度();
    打印("数组1长度: " + 长度);
    
    // 测试数组添加元素
    数组1.添加(6);
    打印("添加6后的数组1: " + 数组1);
    
    // 测试数组删除元素
    数组1.删除(2);
    打印("删除索引2后的数组1: " + 数组1);
    
    // 测试空数组
    let 空数组 = [];
    打印("空数组: " + 空数组);
    
    // 测试数组中的不同类型
    let 混合数组 = [1, "hello", 3.14];
    打印("混合数组: " + 混合数组);
    
    // 测试嵌套数组
    let 嵌套数组 = [[1, 2], [3, 4]];
    打印("嵌套数组: " + 嵌套数组);
    
    // 测试数组边界情况
    let 边界数组 = [1];
    打印("边界数组: " + 边界数组);
    边界数组.添加(2);
    打印("添加后的边界数组: " + 边界数组);
    边界数组.删除(0);
    打印("删除后的边界数组: " + 边界数组);
}

// 调用主函数
主();